{
  "name": "ts-libs",
  "version": "3.2.2-v2",
  "description": "Typescript libs as scoped modules",
  "scripts": {
    "modulify": "rm -fr lib/ && ts-node -P tools/tsconfig.json tools/modulify-libs.ts",
    "test": "bash tests/test.sh",
    "prepublishOnly": "npm run test && npm run modulify"
  },
  "keywords": [
    "typescript",
    "lib",
    "module"
  ],
  "peerDependencies": {
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@types/node": "*",
    "typescript": "3.2.2",
    "ts-node": "*"
  },
  "files": [
    "lib/*.d.ts"
  ],
  "homepage": "https://github.com/mrbar42/ts-libs",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrbar42/ts-libs.git"
  },
